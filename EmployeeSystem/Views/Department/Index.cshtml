@model IEnumerable<DepartmentEntity>

@{
}

<h1>Departments Overview</h1>


<div class="row">
    <div class="col-md-6">
        <form class="d-flex" asp-action="Index" asp-controller="Department" method="get">
        </form>
    </div>
</div>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
            </tr>
        }
    </tbody>
</table>